{
  "name": "leaflet.fullscreen",
  "version": "4.0.0",
  "description": "Simple plugin for Leaflet that adds fullscreen button to your maps.",
  "type": "module",
  "main": "dist/Control.FullScreen.js",
  "module": "dist/Control.FullScreen.js",
  "browser": "dist/Control.FullScreen.umd.js",
  "exports": {
    ".": {
      "import": "./dist/Control.FullScreen.js",
      "require": "./dist/Control.FullScreen.umd.js"
    },
    "./dist/Control.FullScreen.css": "./dist/Control.FullScreen.css"
  },
  "files": [
    "dist/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint && stylelint **/*.css && prettier . --check",
    "lint:fix": "eslint --fix && stylelint **/*.css --fix && prettier . --write",
    "release": "commit-and-tag-version",
    "publish": "git push --follow-tags && npm publish"
  },
  "peerDependencies": {
    "leaflet": "^1.7.0 || >=2.0.0-alpha.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/brunob/leaflet.fullscreen.git"
  },
  "keywords": [
    "leaflet",
    "plugins",
    "maps",
    "fullscreen"
  ],
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@stylistic/eslint-plugin-js": "^4.1.0",
    "commit-and-tag-version": "^12.5.0",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "rollup": "^4.53.3",
    "stylelint": "^16.13.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-prettier": "^5.0.2"
  },
  "author": "b_b",
  "license": "MIT",
  "readmeFilename": "README.md"
}
